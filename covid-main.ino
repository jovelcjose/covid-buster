void setup()
{
  pinMode(uv, OUTPUT); // UV light, pump, valve connected to arduino and acting as output
  pinMode(pump, OUTPUT);
  pinMode(valve, OUTPUT);
  digitalWrite(pump, LOW); // Initialise the hardwares
  digitalWrite(uv, LOW);
  digitalWrite(valve, LOW);
  Serial.begin(9600); // set the baud rate
  delay(5000);

  Serial.print("page ");
  Serial.print(1);
  senddisp(); // initialise display

}


void loop()
{
  currentMillis = millis(); // get the realtime
  hand();
  if (in)
  {
    digitalWrite(uv, LOW);
    point = 1;
    war_count = 0;
    startPlayback(welcom, sizeof(welcom));
    delay(500);
    currentMillis = millis();
    previousMillis = currentMillis;
    digitalWrite(valve, HIGH);

    Serial.print("page ");
    Serial.print(2);
    senddisp();
    while (1)
    {
      currentMillis = millis();
      hand();
      if (!in)
      {
        startPlayback(warning, sizeof(warning));
        delay(500);
        war_count++;
        previousMillis = previousMillis + 500;
        currentMillis = millis();

        if (war_count > 10)
